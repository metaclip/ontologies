    <owl:NamedIndividual rdf:about="http://www.metaclip.org/datasource/datasource.owl#CSDI">
        <rdf:type rdf:resource="http://www.metaclip.org/datasource/datasource.owl#ETCCDI"/>
        <dc:description xml:lang="en">Cold spell duration index: Annual count of days with at least 6 consecutive days when TN &lt; 10th percentile</dc:description>
