        <rdfs:seeAlso rdf:resource="http://www.metaclip.org/datasource/datasource.owl#ENSO1plus2"/>
        <rdfs:seeAlso rdf:resource="http://www.metaclip.org/datasource/datasource.owl#Nino3_4"/>
